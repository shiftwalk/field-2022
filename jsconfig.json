{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/helpers/*": ["helpers/*"],
      "@/styles/*": ["styles/*"],
      "@/public/*": ["public/*"],
      "@/context/*": ["context/*"],
      "@/services/*": ["services/*"],
      "@/icons/*": ["public/icons/*"],
